     elif [ "$D" = "Programas12" ]; then
          Programas12

     elif [ "$D" = "Programas13" ]; then
          Programas13

     else
        if [ $Mordomo = "Mag" ]; then
           ffError
           sleep 1
           clear
           DokaShell
           FPrompt
           Categorias2P1
           FPromptC
