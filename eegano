          apt-get -y update 1> /dev/null 2> /dev/null
          apt-get -y install nipper-ng
          sleep 1
          echo -e $Color"Processo Finalizado."$ColorR
          sleep 1
          echo -e $Color"De um Enter para Restaurar"\
                        "a Interface Padrao."$ColorR
          sleep 1
          PrompTProgramas07

     elif [ "$G" = "8help" ]; then
