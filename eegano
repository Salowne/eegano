          apt -y update
          echo -e $Color"De um Enter para Restaurar"\
                        "a Interface Padrao."$ColorR
          sleep 1
          PPrompt

     elif [ "$L" = "KaliLib" ]; then
          KaliLib

     elif [ "$L" = "Zwdeff" ]; then
          Help_for_my
          sleep 1
