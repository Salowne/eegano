                        "a Interface Padrao."$ColorR
          sleep 1
          PrompTProgramas07

     elif [ "$G" = "x" ]; then
          echo -e $Roxo"INSTALANDO TODAS AS FERRAMENTAS."$Color
          sleep 2
          All_Tools_07
          echo -e $Roxo"TODOS OS PACOTES FORAM INSTALADOS."$ColorR
