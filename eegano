          echo -e $BrancoBAD"Kali: https://www.kali.org/tools/legion/"$ColorR
          echo -e $Color"De um Enter para Restaurar"\
                        "a Interface Padrao."$ColorR
          sleep 1
          PrompTProgramas01

     elif [ "$B" = "65" ]; then
          echo -e $Color"Instalando Programa Xprobe."$Color
