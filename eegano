          echo -e $Branco"o Google. Como exemplo, ele usa a consulta do"$ColorR
          echo -e $Branco"Google abaixo para encontrar todos os arquivos"\
                         ".pdf "$ColorR
          echo -e $Branco"hospedados em example.com e,"\
