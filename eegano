          sleep 0
          exit

     elif [ "$G" = "99" ]; then
          echo -e $Color"Desligamento solicitado...Goodbay..........."$ColorR
          sleep 0
          exit

     elif [ "$G" = "P13" ]; then
          Programas13

     elif [ "$G" = "P12" ]; then
