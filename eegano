echo -e $Verde"(x) (Instalar Todas as ferramentas.)"$ColorR
echo -e "\n\n"
}
PrompTProgramas12(){
echo -e $Color
read -p "$(echo -e "DNF fsociety"$Verde" >"$Color" ")" M
}
