          Categorias2P1
          FPromptC
          Lixera
          PPromptC

     elif [ "$N" = "l" ]; then
          clear
          if [ $Mordomo = "Mag" ]; then
             DokaShell
          fi
          if [ $Mordomo = "Servant" ]; then
             DokaBanner
