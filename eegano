Programas11(){
Programas11B
PrompTProgramas11
if [ "$K" = "01" ]; then
   K="1"
fi
if [ "$K" = "02" ]; then
   K="2"
fi
if [ "$K" = "03" ]; then
   K="3"
fi
if [ "$K" = "04" ]; then
   K="4"
fi
if [ "$K" = "05" ]; then
   K="5"
fi
