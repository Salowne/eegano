}
PrompTProgramas01(){
echo -e $Color
read -p "$(echo -e "DNF fsociety"$Verde" >"$Color" ")" B
}
All_Tools_01(){
apt-get -y update 1> /dev/null 2> /dev/null
apt-get -y install termineter
