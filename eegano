          sleep 1
          echo -e $Color"De um Enter para Restaurar"\
                        "a Interface Padrao."$ColorR
          sleep 1
          PrompTProgramas13

     elif [ "$O" = "6help" ]; then
          echo -e $BrancoBAD"Sobre Smali:"$ColorR
          echo -e $Branco"smali/baksmali e um"$ColorR
