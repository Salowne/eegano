     elif [ "$D" = "back" ]; then
          if [ $Mordomo = "Mag" ]; then
             clear
             DokaShell
             FPrompt
             Categorias
          fi
          if [ $Mordomo = "Servant" ]; then
             clear
             DokaBanner
             FPrompt
             Categorias
          fi
