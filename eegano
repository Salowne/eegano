           Programas09i
        fi
        if [ $Mordomo = "star" ]; then
           ffError
           sleep 1
           clear
           Morning
           FPrompt
           Categorias2P1
           FPromptC
           Programas09B
           FPromptP9
           Programas09i
        fi
      fi
  else
     if [ $Mordomo = "Mag" ]; then
        clear
        DokaShell
        FPrompt
        Categorias2P1
        FPromptC
        Programas09B
        FPromptP9
        Programas09i
     fi
     if [ $Mordomo = "Servant" ]; then
        clear
        DokaBanner
        FPrompt
        Categorias2P1
        FPromptC
        Programas09B
        FPromptP9
        Programas09i
     fi
     if [ $Mordomo = "star" ]; then
        clear
        Morning
        FPrompt
        Categorias2P1
        FPromptC
        Programas09B
        FPromptP9
        Programas09i
     fi
   fi

if [ $Mordomo = "Mag" ]; then
   clear
   DokaShell
   FPrompt
   Categorias2P1
   FPromptC
   Programas09B
   FPromptP9
   Programas09i
fi
if [ $Mordomo = "Servant" ]; then
   clear
   DokaBanner
   FPrompt
   Categorias2P1
   FPromptC
   Programas09B
   FPromptP9
   Programas09i
fi
if [ $Mordomo = "star" ]; then
   clear
   Morning
   FPrompt
   Categorias2P1
   FPromptC
   Programas09B
   FPromptP9
  Programas09i
fi
done
if [ $Mordomo = "Mag" ]; then
   ffError
   sleep 1
   clear
   DokaShell
   FPrompt
   Categorias2P1
   FPromptC
   Programas09B
   FPromptP9
   Programas09i
fi
