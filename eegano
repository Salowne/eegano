          echo -e $Color"De um Enter para Restaurar"\
                        "a Interface Padrao."$ColorR
          sleep 1
          PrompTProgramas07

     elif [ "$G" = "back" ]; then
          if [ $Mordomo = "Mag" ]; then
             clear
             DokaShell
             FPrompt
             Categorias
          fi
          if [ $Mordomo = "Servant" ]; then
             clear
             DokaBanner
             FPrompt
             Categorias
          fi
          if [ $Mordomo = "star" ]; then
             clear
             Morning
             FPrompt
             Categorias
          fi

     elif [ "$G" = "Back" ]; then
          if [ $Mordomo = "Mag" ]; then
             clear
             DokaShell
             FPrompt
             Categorias
          fi
          if [ $Mordomo = "Servant" ]; then
             clear
             DokaBanner
             FPrompt
             Categorias
          fi
          if [ $Mordomo = "star" ]; then
             clear
