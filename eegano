          echo -e $Color"De um Enter para Restaurar"\
                        "a Interface Padrao."$ColorR
          sleep 1
          LixeraPrompt

     elif [ "$LIXO" = "P14" ]; then
          Programas13

     elif [ "$LIXO" = "P12" ]; then
          Programas12

     elif [ "$LIXO" = "P11" ]; then
          Programas11

