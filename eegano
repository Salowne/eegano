   O="9"
fi
while [ "$O" ];
do
  if [ "$O" ]; then
     if [ "$O" = "1" ]; then
        echo -e $Color"Instalando Programa Android-SDK."$Color
        apt-get -y update 1> /dev/null 2> /dev/null
